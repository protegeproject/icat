<module rename-to="webprotege">

	<!-- Inherit the core Google Web Toolkit stuff -->
	<inherits name="com.google.gwt.user.User"/>
	
	<inherits name="com.google.gwt.resources.Resources" />
	
	<!-- Inherit the GWT-Ext Toolkit stff -->
	<inherits name='com.gwtext.GwtExt' />
	
	<inherits name='com.googlecode.gwt.crypto.Crypto'/>
	<inherits name="com.google.gwt.http.HTTP"/>

	<!-- Inhert the GWT default style -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/>  -->	

	<entry-point class="edu.stanford.bmir.protege.web.client.WebProtege"/> 
			
	<servlet path="/admin" class="edu.stanford.bmir.protege.web.server.AdminServiceImpl"/>
	<servlet path="/notification" class="edu.stanford.bmir.protege.web.server.NotificationServiceImpl"/>
	<servlet path="/ontology" class="edu.stanford.bmir.protege.web.server.OntologyServiceImpl"/>
	<servlet path="/projectconfig" class="edu.stanford.bmir.protege.web.server.ProjectConfigurationServiceImpl"/>
	<servlet path="/chao" class="edu.stanford.bmir.protege.web.server.ChAOServiceImpl"/>
	<servlet path="/chaostats" class="edu.stanford.bmir.protege.web.server.ChAOStatsServiceImpl"/>
	<servlet path="/icd" class="edu.stanford.bmir.protege.web.server.ICDServiceImpl"/>
    <servlet path="/hierarchy" class="edu.stanford.bmir.protege.web.server.HierarchyServiceImpl"/>
    <servlet path="/openid" class="edu.stanford.bmir.protege.web.server.OpenIdServiceImpl"/>
    <servlet path="/bpProposals" class="edu.stanford.bmir.protege.web.server.bioportal.BioPortalProposalsImpl"/>
    <servlet path="/bpUpload" class="edu.stanford.bmir.protege.web.server.bioportal.BioPortalUploadImpl" />
    <servlet path="/bpaccess" class="edu.stanford.bmir.protege.web.server.bioportal.BioPortalAccessImpl" />
    <servlet path="/scripting" class="edu.stanford.bmir.protege.web.server.scripting.ScriptingServiceImpl" />
    
    
</module>
