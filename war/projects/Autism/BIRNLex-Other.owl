    <!-- 
          Uncessary import.  Comes in via OBO-UBO
          
          <owl:imports rdf:resource="http://purl.org/nbirn/birnlex/ontology/obo-foundry/core/obo-foundry-core-full-import.owl"/>
          
      -->
    
  </owl:Ontology>
    
