  xmlns:birn_obo_ubo="http://purl.org/nbirn/birnlex/ontology/BIRNLex-OBO-UBO.owl#"
  xmlns:birn_org_tax="http://purl.org/nbirn/birnlex/ontology/BIRNLex-OrganismalTaxonomy.owl#"
