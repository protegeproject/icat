<?xml version="1.0"?>
<assembly>
    <id>bin</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <moduleSets>
        <moduleSet>
            <includes>
                <!-- Modules to include in assembly -->
                <include>owlapi:api</include>
                <include>owlapi:apibinding</include>
                <include>owlapi:debugging</include>
                <include>owlapi:dig1_1</include>
                <include>owlapi:functionalparser</include>
                <include>owlapi:functionalrenderer</include>
                <include>owlapi:impl</include>
                <include>owlapi:krssparser</include>
                <include>owlapi:oboparser</include>
                <include>owlapi:oborenderer</include>
                <include>owlapi:owlxmlparser</include>
                <include>owlapi:owlxmlrenderer</include>
                <include>owlapi:rdfapi</include>
                <include>owlapi:rdfxmlparser</include>
                <include>owlapi:rdfxmlrenderer</include>
                <include>owlapi:util</include>
                <include>owlapi:metrics</include>
                <include>owlapi:change</include>
                <include>owlapi:mansyntaxparser</include>
                <include>owlapi:mansyntaxrenderer</include>
            </includes>
            <binaries>
                <includeDependencies>false</includeDependencies>
                <unpack>false</unpack>
                <outputFileNameMapping>owlapi-${artifactId}.${extension}</outputFileNameMapping>
                 <dependencySets>
                    <dependencySet>
                        <outputDirectory>/</outputDirectory>
                        <includes>
                            <include>commons-lang:commons-lang</include>
                            <include>antlr-runtime:antlr-runtime</include>
                        </includes>
                    </dependencySet>
                </dependencySets>
            </binaries>
        </moduleSet>
    </moduleSets>
</assembly>